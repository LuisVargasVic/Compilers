import java.util.ArrayList;
import java.util.List;

public class Error {
	public String id;
	public List<Character> errorType = new ArrayList<Character>();
	public Integer position;
	public Integer line;
	
	public Error(String id, List<Character> errorType, Integer position, Integer line) {
		super();
		this.id = id;
		this.errorType = errorType;
		this.position = position;
		this.line = line;
	}
}
