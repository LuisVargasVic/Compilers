public class Token {
	public String id;
	public String type;
	public String dataType;
	public Integer initCharacter;
	public Integer line;
	
	public Token(String id, String type, String dataType, Integer initCharacter, Integer line) {
		super();
		this.id = id;
		this.type = type;
		this.dataType = dataType;
		this.initCharacter = initCharacter;
		this.line = line;
	}
}
